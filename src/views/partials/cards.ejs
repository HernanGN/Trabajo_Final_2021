<% dataProgramas.forEach((prog)=>{ %>
    <div class="<%= prog.clase %>">
        <article>    
            <div class="card" style="width: 100%;">
                <img src="<%= prog.imagen %>" class="card-img-top" alt="Imagen <%= prog.titulo %>">
                <div class="card-body">
                    <h5 class="card-title text-center"><%= prog.titulo %></h5>
                    <p class="card-text"><%= prog.texto %></p>
                    <div class="col-md-12 text-center">
                        <a href="#" class="btn btn-primary">Detalles del <%= prog.titulo %></a>
                    </div>
                </div>
            </div>
        </article>
    </div>
<% }); %>